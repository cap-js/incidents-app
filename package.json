{
  "name": "@capire/incidents",
  "version": "1.0.0",
  "calesi": {
    "@cap-js/change-tracking": "*",
    "@cap-js/notifications": "*",
    "@cap-js/attachments": "*",
    "@cap-js/telemetry": "*",
    "@sap/ams": "*",
    "@sap/ams-dev": "*",
    "dummy": 0
  },
  "dependencies": {
   "@cap-js/attachments": "^2.1.2",
    "@cap-js/hana": "^1",
    "@sap-cloud-sdk/connectivity": "^3.26.4",
    "@sap-cloud-sdk/http-client": "^3.26.4",
    "@sap-cloud-sdk/resilience": "^3.26.4",
    "@sap-cloud-sdk/util": "^3.26.4",
    "@sap/cds": "^8.9.4",
    "@sap/xssec": "^4",
    "express": "^4"
  },
  "devDependencies": {

     "@cap-js/cds-types": "^0.10.0",
      "@cap-js/sqlite": "^1",
      "@sap/cds-dk": ">=8"
  },
  "scripts": {
    "watch": "cds watch",
    "start": "cds-serve",
    "test": "npx jest --silent"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/xmpls/"
    ]
  },
  "sapux": [
    "app/incidents"
  ],
  "engines": {
    "node": "^22"
  },
  "cds": {
    "requires": {
      "auth": {
        "[development]": {
          "kind":"basic",
          "users": {
            "alice": {
              "roles": [
                "support",
                "admin"
              ]
            },
            "bob": {
              "roles": [
                "support"
              ]
            }
          }
        }
      },
      "destinations": true,
      "html5-repo": true,
      "workzone": true,
      "[production]": {
        "db": "hana",
        "auth": {
          "kind": "xsuaa"
        }
      },
      "html5-runtime": true
    },
    "sql": {
      "native_hana_associations": false
    }
  },
  "attachments": {
    "scan": true
  },
  "private": true
}
