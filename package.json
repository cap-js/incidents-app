{
  "name": "@capire/incidents",
  "version": "1.0.0",
  "calesi": {
    "@cap-js/change-tracking": "*",
    "@cap-js/notifications": "*",
    "@cap-js/attachments": "*",
    "@cap-js/telemetry": "*",
    "@sap/ams": "*",
    "@sap/ams-dev": "*",
    "dummy": 0
  },
  "dependencies": {
    "@cap-js/hana": "^1",
    "@cap-js/attachments": "git+https://github.com/cap-js/attachments#fix/non-draft_mtls_upload",
    "@sap/cds": ">=8",
    "@sap/cds-mtxs": "^2",
    "@sap/xssec": "^4",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/cds-test": "^0",
    "@cap-js/change-tracking": "^1.0.6",
    "@cap-js/sqlite": "^1.0.1",
    "@sap/cds-dk": "^8",
    "@sap/ux-specification": "^1.108.4"
  },
  "scripts": {
    "watch": "cds watch",
    "start": "cds-serve",
    "test": "npx jest --silent"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/xmpls/"
    ]
  },
  "sapux": [
    "app/incidents"
  ],
  "cds": {
    "requires": {
      "auth": {
        "[development]": {
          "users": {
            "alice": {
              "roles": [
                "support",
                "admin"
              ]
            },
            "bob": {
              "roles": [
                "support"
              ]
            }
          }
        },
        "kind": "xsuaa"
      },
      "destinations": true,
      "html5-repo": true,
      "workzone": true,
      "[production]": {
        "db": "hana",
        "auth": {
          "kind": "xsuaa"
        },
        "multitenancy": true
      },
      "html5-runtime": true
    },
    "sql": {
      "native_hana_associations": false
    },
    "profile": "with-mtx-sidecar"
  },
  "attachments": {
    "scan": true
  },
  "private": true
}
